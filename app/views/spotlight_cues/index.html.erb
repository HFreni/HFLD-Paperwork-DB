<p id="notice"><%= notice %></p>

<h1>Spotlight Cues</h1>

<table>
  <thead>
    <tr>
      <th>Color</th>
      <th>Size</th>
      <th>Intensity</th>
      <th>Action</th>
      <th>Time</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @spotlight_cues.each do |spotlight_cue| %>
      <tr>
        <td><%= spotlight_cue.color %></td>
        <td><%= spotlight_cue.size %></td>
        <td><%= spotlight_cue.intensity %></td>
        <td><%= spotlight_cue.action %></td>
        <td><%= spotlight_cue.time %></td>
        <td><%= spotlight_cue.description %></td>
        <td><%= link_to 'Show', spotlight_cue %></td>
        <td><%= link_to 'Edit', edit_spotlight_cue_path(spotlight_cue) %></td>
        <td><%= link_to 'Destroy', spotlight_cue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Spotlight Cue', new_spotlight_cue_path %>
